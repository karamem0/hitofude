{
  "name": "@karamem0/hitofude",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=default --reporter=junit --coverage",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "stylelint": "stylelint src/**/*.{css,scss,jsx,tsx}",
    "stylelint:fix": "stylelint src/**/*.{css,scss,jsx,tsx} --fix",
    "formatjs:extract": "formatjs extract src/**/messages.ts --out-file src/translations/en.json --id-interpolation-pattern [sha512:contenthash:base64:6] --format simple",
    "formatjs:compile": "npm run formatjs:compile-en && npm run formatjs:compile-ja",
    "formatjs:compile-en": "formatjs compile src/translations/en.json --out-file src/translations/compiled/en.json --ast --format simple",
    "formatjs:compile-ja": "formatjs compile src/translations/ja.json --out-file src/translations/compiled/ja.json --ast --format simple"
  },
  "dependencies": {
    "@automapper/core": "^8.8.1",
    "@automapper/pojos": "^8.8.1",
    "@azure/msal-browser": "^5.0.2",
    "@azure/msal-react": "^5.0.2",
    "@fluentui/react-components": "^9.72.10",
    "@fluentui/react-icons": "^2.0.317",
    "@microsoft/applicationinsights-react-js": "^19.3.8",
    "@microsoft/applicationinsights-web": "^3.3.11",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "bytes": "^3.1.2",
    "dexie": "^4.2.1",
    "mermaid": "^11.12.2",
    "mime": "^4.1.0",
    "monaco-editor": "^0.55.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.1.0",
    "react-hook-form": "^7.71.1",
    "react-icons": "^5.5.0",
    "react-intl": "^8.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-use": "^17.6.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/jest": "^11.14.2",
    "@emotion/react": "^11.14.0",
    "@eslint/compat": "^2.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@formatjs/cli": "^6.12.0",
    "@microsoft/microsoft-graph-types": "^2.43.1",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bytes": "^3.1.5",
    "@types/mime": "^4.0.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-plugin-formatjs": "^11.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-testing-library": "^7.15.4",
    "globals": "^17.1.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "postcss-styled-syntax": "^0.7.1",
    "stylelint": "^17.0.0",
    "stylelint-config-recess-order": "^7.6.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "lodash-es": "^4.17.23"
  }
}

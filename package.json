{
  "name": "hitofude",
  "private": true,
  "version": "1.4.1",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss,jsx,tsx}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss,jsx,tsx}' --fix",
    "formatjs:extract": "formatjs extract 'src/**/messages.ts' --out-file 'src/translations/en.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "formatjs:compile": "npm run formatjs:compile-en && npm run formatjs:compile-ja",
    "formatjs:compile-en": "formatjs compile 'src/translations/en.json' --out-file 'src/translations/compiled/en.json' --ast --format simple",
    "formatjs:compile-ja": "formatjs compile 'src/translations/ja.json' --out-file 'src/translations/compiled/ja.json' --ast --format simple"
  },
  "dependencies": {
    "@automapper/core": "^8.7.7",
    "@automapper/pojos": "^8.7.7",
    "@azure/msal-browser": "^2.37.0",
    "@azure/msal-react": "^1.5.7",
    "@fluentui/react-components": "^9.20.0",
    "@fluentui/react-icons-mdl2": "^1.3.41",
    "@fluentui/react-icons-mdl2-branded": "^1.2.42",
    "@microsoft/applicationinsights-react-js": "^3.4.1",
    "@microsoft/applicationinsights-web": "^2.8.10",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "bytes": "^3.1.2",
    "monaco-editor": "^0.38.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-hook-form": "^7.43.9",
    "react-intl": "^6.4.2",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.11.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "remark-gfm": "^3.0.1",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@formatjs/cli": "^6.0.4",
    "@microsoft/microsoft-graph-types": "^2.31.0",
    "@types/bytes": "^3.1.1",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-plugin-formatjs": "^10.4.0",
    "eslint": "^8.37.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "postcss-styled-syntax": "^0.4.0",
    "sass": "^1.60.0",
    "stylelint": "^15.3.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.5"
  },
  "volta": {
    "node": "16.19.0"
  }
}

{
  "name": "hitofude",
  "private": true,
  "version": "1.9.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss,jsx,tsx}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss,jsx,tsx}' --fix",
    "formatjs:extract": "formatjs extract 'src/**/messages.ts' --out-file 'src/translations/en.json' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "formatjs:compile": "npm run formatjs:compile-en && npm run formatjs:compile-ja",
    "formatjs:compile-en": "formatjs compile 'src/translations/en.json' --out-file 'src/translations/compiled/en.json' --ast --format simple",
    "formatjs:compile-ja": "formatjs compile 'src/translations/ja.json' --out-file 'src/translations/compiled/ja.json' --ast --format simple"
  },
  "dependencies": {
    "@automapper/core": "^8.7.7",
    "@automapper/pojos": "^8.7.7",
    "@azure/msal-browser": "^3.1.0",
    "@azure/msal-react": "^2.0.3",
    "@fluentui/react-components": "^9.30.4",
    "@fluentui/react-icons-mdl2": "^1.3.49",
    "@fluentui/react-icons-mdl2-branded": "^1.2.51",
    "@microsoft/applicationinsights-react-js": "^17.0.0",
    "@microsoft/applicationinsights-web": "^3.0.2",
    "@microsoft/microsoft-graph-client": "^3.0.6",
    "bytes": "^3.1.2",
    "mime": "^3.0.0",
    "monaco-editor": "^0.41.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.46.1",
    "react-intl": "^6.4.6",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.15.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "remark-gfm": "^3.0.1",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@formatjs/cli": "^6.1.5",
    "@microsoft/microsoft-graph-types": "^2.38.0",
    "@types/bytes": "^3.1.1",
    "@types/mime": "^3.0.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-plugin-formatjs": "^10.5.5",
    "eslint": "^8.48.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.29",
    "postcss-scss": "^4.0.8",
    "postcss-styled-syntax": "^0.4.0",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "volta": {
    "node": "18.17.1"
  }
}
